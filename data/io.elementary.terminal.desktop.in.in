[Desktop Entry]
_Name=Terminal
_GenericName=Terminal
_Comment=Use the command line
TryExec=@EXEC_NAME@
Exec=@EXEC_NAME@
Icon=utilities-terminal
Type=Application
Categories=GNOME;GTK;System;TerminalEmulator;
StartupNotify=true
X-GNOME-Gettext-Domain=@GETTEXT_PACKAGE@
_Keywords=command;prompt;cmd;commandline;run;
X-GNOME-UsesNotifications=true
Actions=NewWindow;NewRootWindow;

[Desktop Action NewWindow]
_Name=New Window
Icon=utilities-terminal
Exec=@EXEC_NAME@

[Desktop Action NewRootWindow]
_Name=New Root Tab
Icon=utilities-terminal
Exec=@EXEC_NAME@ -e "sudo -s"
