msgid "Minor bug fixes"
msgstr "Mniejsze poprawki błędów"

#: data/io.elementary.terminal.appdata.xml.in:211
msgid "FISH integration fixes"
msgstr "Poprawki dla integracji z FISHem"

#: data/io.elementary.terminal.appdata.xml.in:212
msgid "Theme preference menu"
msgstr "Menu ustawień motywu"

#: data/io.elementary.terminal.appdata.xml.in:213
msgid "Zoom level controls"
msgstr "Kontrola poziomu przybliżenia"

#: data/io.elementary.terminal.appdata.xml.in:230
msgid "Allow tab to close after typing exit at the commandline"
msgstr "Możliwość zamknięcia karty przez wpisanie exit w linii poleceń"

#: data/io.elementary.terminal.appdata.xml.in:239
msgid "Fix copying URLs to clipboard"
msgstr "Naprawiono kopiowanie URL do schowka"

#: data/io.elementary.terminal.appdata.xml.in:256
msgid "Fix appstream date format"
msgstr "Naprawiono format daty appstream"

