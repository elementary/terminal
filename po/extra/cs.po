msgid "Minor bug fixes"
msgstr "Opravy drobných chyb"

#: data/io.elementary.terminal.appdata.xml.in:268
msgid "FISH integration fixes"
msgstr "Opravy propojení se shellem FISH"

#: data/io.elementary.terminal.appdata.xml.in:269
msgid "Theme preference menu"
msgstr "Nabídka pro předvolby motivu vzhledu"

#: data/io.elementary.terminal.appdata.xml.in:270
msgid "Zoom level controls"
msgstr "Ovládací prvky pro změnu úrovně přiblížení"

#: data/io.elementary.terminal.appdata.xml.in:287
msgid "Allow tab to close after typing exit at the commandline"
msgstr "Karty už se zavírají po zadání exit v příkazovém řádku"

#: data/io.elementary.terminal.appdata.xml.in:296
msgid "Fix copying URLs to clipboard"
msgstr "Opraveno kopírování URL adres do schránky"

#: data/io.elementary.terminal.appdata.xml.in:313
msgid "Fix appstream date format"
msgstr "Opraven formát data v appstream metadatech"

