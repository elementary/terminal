msgid "FISH integration fixes"
msgstr "FISH tümleştirmesi düzeltildi"

#: data/io.elementary.terminal.appdata.xml.in:232
msgid "Theme preference menu"
msgstr "Tema tercih menüsü"

#: data/io.elementary.terminal.appdata.xml.in:233
msgid "Zoom level controls"
msgstr "Yakınlaştırma düzeyi kontrolleri"

#: data/io.elementary.terminal.appdata.xml.in:250
msgid "Allow tab to close after typing exit at the commandline"
msgstr "Komut satırına exit yazdıktan sonra sekmenin kapanmasına izin ver"

#: data/io.elementary.terminal.appdata.xml.in:259
msgid "Fix copying URLs to clipboard"
msgstr "URL'lerin panoya kopyalanması düzeltildi"

#: data/io.elementary.terminal.appdata.xml.in:276
msgid "Fix appstream date format"
msgstr "appstream uygulamasının tarih biçimi düzeltildi"

