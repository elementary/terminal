msgid "Minor bug fixes"
msgstr "修正了一些小错误"

#: data/io.elementary.terminal.appdata.xml.in:214
msgid "FISH integration fixes"
msgstr "FISH 集成修复"

#: data/io.elementary.terminal.appdata.xml.in:215
msgid "Theme preference menu"
msgstr "主题首选项菜单"

#: data/io.elementary.terminal.appdata.xml.in:216
msgid "Zoom level controls"
msgstr "缩放等级控制"

#: data/io.elementary.terminal.appdata.xml.in:233
msgid "Allow tab to close after typing exit at the commandline"
msgstr "允许键入 exit 命令行关闭选项卡"

#: data/io.elementary.terminal.appdata.xml.in:242
msgid "Fix copying URLs to clipboard"
msgstr "修复 URL 复制问题"

#: data/io.elementary.terminal.appdata.xml.in:259
msgid "Fix appstream date format"
msgstr "修正 Appstream 日期格式"

