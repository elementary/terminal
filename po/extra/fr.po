#: data/io.elementary.terminal.appdata.xml.in:138
msgid "Theme preference menu"
msgstr "Menu de préférence du thème"

#: data/io.elementary.terminal.appdata.xml.in:139
msgid "Zoom level controls"
msgstr "Contrôles du niveau de zoom"

#: data/io.elementary.terminal.appdata.xml.in:156
msgid "Allow tab to close after typing exit at the commandline"
msgstr ""
"Permet aux onglets d'être fermés après avoir tapé exit en ligne de commande"

#: data/io.elementary.terminal.appdata.xml.in:165
msgid "Fix copying URLs to clipboard"
msgstr "Correction de la copie des URL dans le presse-papiers"

#: data/io.elementary.terminal.appdata.xml.in:182
msgid "Fix appstream date format"
msgstr "Correction du format de date dans les métadonnées"

