msgid "Minor bug fixes"
msgstr "Correcciones de errores menores"

#: data/io.elementary.terminal.appdata.xml.in:195
msgid "FISH integration fixes"
msgstr "Correcciones de integración FISH"

#: data/io.elementary.terminal.appdata.xml.in:196
msgid "Theme preference menu"
msgstr "Menú de preferencias de tema"

#: data/io.elementary.terminal.appdata.xml.in:197
msgid "Zoom level controls"
msgstr "Controles de nivel de escala"

#: data/io.elementary.terminal.appdata.xml.in:214
msgid "Allow tab to close after typing exit at the commandline"
msgstr ""
"Permitir que la pestaña se cierre después de escribir exit en la línea de "
"comandos"

#: data/io.elementary.terminal.appdata.xml.in:223
msgid "Fix copying URLs to clipboard"
msgstr "Corrección de copia de URL al portapapeles"

#: data/io.elementary.terminal.appdata.xml.in:240
msgid "Fix appstream date format"
msgstr "Corregir el formato de fecha de appstream"

