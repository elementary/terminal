msgid "Minor bug fixes"
msgstr "軽微なバグの修正"

#: data/io.elementary.terminal.appdata.xml.in:280
msgid "FISH integration fixes"
msgstr "fish との統合の修正"

#: data/io.elementary.terminal.appdata.xml.in:281
msgid "Theme preference menu"
msgstr "テーマ設定メニューを追加"

#: data/io.elementary.terminal.appdata.xml.in:282
msgid "Zoom level controls"
msgstr "拡大レベルのコントロールを追加"

#: data/io.elementary.terminal.appdata.xml.in:299
msgid "Allow tab to close after typing exit at the commandline"
msgstr "コマンドラインで \"exit\" の入力後にタブを閉じられるように修正"

#: data/io.elementary.terminal.appdata.xml.in:308
msgid "Fix copying URLs to clipboard"
msgstr "クリップボードへ URL のコピーする際の不具合を修正"

#: data/io.elementary.terminal.appdata.xml.in:325
msgid "Fix appstream date format"
msgstr "AppStream データ形式を修正"

#: data/io.elementary.terminal.desktop.in:13
msgid "command;prompt;cmd;commandline;run;"
msgstr ""
"command;prompt;cmd;commandline;run;コマンド;プロンプト;コマンドライン;実行;"

